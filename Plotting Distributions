#Plot of Chisquare 
x = rchisq(50,10)
hist(x,probability = TRUE,main = "Chisquare Distribution")
lines(density(x),col=2)

#Normal Distribution 
x = rnorm(200)
hist(x, probability = TRUE, main = "Normal Distribution")
lines(density(x),col=3)

x=rchisq(200,5)
hist(x,freq = FALSE)
curve(dchisq(x,5),add = TRUE,col=3)
